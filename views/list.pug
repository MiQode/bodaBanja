doctype html
html
    head
        title List of User
        link(rel="stylesheet", href="./style.css")
    body
        table
            thead
                tr
                    th First Name
                    th Last Name
                    th Nick Name
            tbody
                each user in users
                    tr
                        td #{user.firstName}
                        td #{user.lastName}
                        td #{user.nickName}

